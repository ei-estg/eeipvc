{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./build/**"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "lint": {

    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"]
    }
  },
  "globalDependencies": [
    ".env",
    "*tsconfig.json",
    ".eslintrc.json"
  ]
}
